pipeline {
  agent any
  stages {
    stage('Sync') {
      steps {
        echo 'Sync'
        sh 'echo "Sync"'
      }
    }
    stage('Build') {
      steps {
        echo 'Build'
        sh 'echo "Build"'
      }
    }
    stage('Archive') {
      steps {
        echo 'Archive'
        sh 'echo "Archive"'
      }
    }
    stage('Deliver') {
      steps {
        echo 'Deliver'
        sh 'echo "Deliver"'
      }
    }
  }
}